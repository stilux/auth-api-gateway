		{
			"name": "Get Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "6aea145e-fc66-4c12-bcf2-ad59340366ca",
						"exec": [
							"pm.test(\"Check status\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"if (pm.response.to.have.status(200)) {\r",
							"    try {\r",
							"        var tokenResponse = JSON.parse(responseBody);\r",
							"        console.log(tokenResponse.access_token);\r",
							"        pm.collectionVariables.set(\"access_token\", tokenResponse.access_token);\r",
							"    }\r",
							"    catch (e) {\r",
							"        console.log(e);\r",
							"    }\r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"username\": \"{{username}}\",\r\n    \"password\": \"{{password}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						},
						"urlencoded": {}
					}
				},
				"url": {
					"raw": "{{base_url}}/auth/token",
